{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)\\.(tsx|ts)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.(?:tsx|ts|js|jsx|css|json|png|jpg|jpeg|svg|gif|webp|ico)$).*)",
      "destination": "/index.html"
    }
  ]
}